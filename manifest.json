{
    "name": "Desmos Scientific Calculator",
    "version": "1.0",
    "description": "",
    "permissions": [],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/img16.png",
        "32": "img/img32.png",
        "48": "img/img48.png",
        "128": "img/img128.png"
      }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "img/img16.png",
        "32": "img/img32.png",
        "48": "img/img48.png",
        "128": "img/img128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+M",
                "mac": "MacCtrl+Command+C"
        }
    }
    },
    "manifest_version": 2
}
